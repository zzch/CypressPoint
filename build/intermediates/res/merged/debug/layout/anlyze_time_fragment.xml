<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
     >

    <TextView
        android:id="@+id/anlyze_tishi"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:gravity="center_horizontal"
        android:text="@string/count_text"
        android:textColor="@color/hei5" />

    <RelativeLayout
        android:id="@+id/anlyze_time_start_relative"
        android:layout_width="match_parent"
        android:layout_height="@dimen/count_time_height"
        android:layout_below="@+id/anlyze_tishi"
        android:layout_marginTop="20dp" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="36dp"
            android:text="开始时间"
            android:textColor="@color/hei2"
            android:textSize="@dimen/txsize_small_xx" />

        <TextView
            android:id="@+id/anlyze_time_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/textView1"
            android:layout_alignBottom="@+id/textView1"
            android:layout_marginLeft="54dp"
            android:layout_toRightOf="@+id/textView1"
            android:text="2013-12-08"
            android:textColor="@color/chengse"
            android:textSize="@dimen/txsize_small_xxx" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/icon_arrow" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="@dimen/count_time_height"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView1"
            android:background="@color/fengexian" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:background="@color/fengexian" />

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/fengexian" />
    </RelativeLayout>
    <Button
        android:id="@+id/anlye_time_sousuo"
        android:layout_width="match_parent"
        android:layout_height="@dimen/view_sss_e"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="#09850c"
        android:text="确定"
        android:textColor="@color/white"
        android:textSize="@dimen/txsize_mid_x" />

    <RelativeLayout
        android:id="@+id/anlyze_time_end_relative"
        android:layout_width="match_parent"
        android:layout_height="@dimen/count_time_height"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/anlyze_time_start_relative" >

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_centerVertical="true"
            android:layout_marginLeft="36dp"
            android:text="结束时间"
            android:textColor="@color/hei2"
            android:textSize="@dimen/txsize_small_xx" />

        <TextView
            android:id="@+id/anlyze_time_end"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_marginLeft="50dp"
            android:layout_toRightOf="@+id/textView2"
            android:text="2013-12-08"
            android:textColor="@color/hei2"
            android:textSize="@dimen/txsize_small_xxx" />

        <ImageView
            android:layout_width="30dp"
            android:layout_height="20dp"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:src="@drawable/icon_arrow" />

        <View
            android:layout_width="0.5dp"
            android:layout_height="@dimen/count_time_height"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="20dp"
            android:layout_toRightOf="@+id/textView2"
            android:background="@color/fengexian" />

        
         <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/fengexian" />
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/anlyze_time_end_relative"
        android:background="@color/fengexian" />

</RelativeLayout><!-- From: file:/D:/Development/Projects/CypressPoint/res/layout/anlyze_time_fragment.xml -->