<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@anim/pingpu"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="72dp"
        android:background="@anim/title_pingpu" >

        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:text="@string/playset"
            android:layout_marginTop="10dp"
            android:textColor="@color/gold"
            android:textSize="@dimen/txsize_mid_x" />

        <Button
            android:id="@+id/count_start"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_marginRight="10dp"
            android:layout_marginTop="13dp"
            android:background="@null"
            android:text="@string/count_fenxi"
            android:textColor="@color/gold"
            android:textSize="@dimen/txsize_small_xx" />
    </RelativeLayout>




    <GridView
        android:id="@+id/gridView1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:numColumns="2" >

    </GridView>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
		
        <FrameLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" 
            android:layout_marginLeft="@dimen/count_frame"  
                    
            >

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" >

                <ImageView
                    android:id="@+id/count_image"
                    android:layout_width="200dp"
                    android:layout_height="200dp"
                    android:layout_marginTop="@dimen/count_image_top"
                    android:layout_marginLeft="@dimen/count_image_left"
                    android:src="@drawable/white" />
            </RelativeLayout>

            <cn.com.zcty.ILovegolf.tools.ChartView
                android:id="@+id/chartView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
               android:layout_marginTop="@dimen/count_ccc"
                />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <ImageView
                    android:layout_width="65dp"
                    android:layout_height="65dp"
                    android:layout_marginLeft="@dimen/count_orange_left"
                    android:layout_marginTop="@dimen/count_orange_top"
                    android:src="@drawable/orange" />
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content" >

                <TextView
                    android:id="@+id/count_average"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/count_text_left"
                    android:layout_marginTop="@dimen/count_text_top"
                    android:textColor="@color/gold" />
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/count_avg_left"
                    android:layout_marginTop="@dimen/count_avg_top"
                    android:text="@string/count_avg"
                    android:textColor="@color/gold_70" />
            </RelativeLayout>
        </FrameLayout>
        <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="230dp"
         >

        <RelativeLayout 
            android:id="@+id/re1"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            >
            <ImageView 
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/e_card_yuan"
                />
            <TextView 
                android:id="@+id/double_eagle_percentage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:textColor="@color/gold"
                android:layout_marginLeft="40dp"
                android:text="10%"
                />
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17dp"
                android:layout_marginLeft="90dp"
                android:layout_marginTop="3dp"
                android:textColor="@color/gold_70"
                android:text="@string/xintianwen"
                />
        </RelativeLayout>
		 <RelativeLayout 
		    android:id="@+id/re2"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
			android:layout_marginLeft="170dp"
	            >
            <ImageView 
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/e_card_yuan"
                />
            <TextView 
                android:id="@+id/eagle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:layout_marginLeft="40dp"
                android:textColor="@color/gold"
                android:text="10%"
                />
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17dp"
                android:textColor="@color/gold_70"
                android:layout_marginLeft="90dp"
                android:layout_marginTop="3dp"
                android:text="@string/laoyin"
                />
        </RelativeLayout>
        <RelativeLayout 
		    android:id="@+id/re3"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/re1"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            >
            <ImageView 
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/e_card_yuan"
                />
            <TextView 
                android:id="@+id/birdie"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:layout_marginLeft="40dp"
                android:textColor="@color/gold"
                android:text="10%"
                />
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17dp"
                android:layout_marginLeft="90dp"
                android:textColor="@color/gold_70"
                android:layout_marginTop="3dp"
                android:text="@string/xiaoniao"
                />
        </RelativeLayout>
        <RelativeLayout 
		    android:id="@+id/re4"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/re2"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="10dp"
            >
            <ImageView 
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/e_card_yuan"
                />
            <TextView 
                android:id="@+id/par"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:textColor="@color/gold"
                android:layout_marginLeft="40dp"
                android:text="10%"
                />
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17dp"
                android:textColor="@color/gold_70"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="90dp"
                android:text="@string/biaozhungan"
                />
        </RelativeLayout>
        <RelativeLayout 
		    android:id="@+id/re5"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/re3"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            >
            <ImageView 
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginTop="5dp"
                android:layout_marginLeft="10dp"
                android:src="@drawable/e_card_yuan"
                />
            <TextView 
                android:id="@+id/bogey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:layout_marginLeft="40dp"
                android:textColor="@color/gold"
                android:text="10%"
                />
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17dp"
                android:textColor="@color/gold_70"
                android:layout_marginTop="3dp"
                android:layout_marginLeft="90dp"
                android:text="@string/baiji"
                />
        </RelativeLayout>
        <RelativeLayout 
		    android:id="@+id/re6"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/re4"
            android:layout_marginLeft="170dp"
            android:layout_marginTop="10dp"
            >
            <ImageView 
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="5dp"
                android:src="@drawable/e_card_yuan"
                />
            <TextView 
                android:id="@+id/double_bogey"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="22dp"
                android:layout_marginLeft="40dp"
                android:textColor="@color/gold"
                android:text="10%"
                />
            <TextView 
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="17dp"
                android:layout_marginLeft="90dp"
                android:textColor="@color/gold_70"
                android:layout_marginTop="3dp"
                android:text="@string/shuangbaiji"
                />
        </RelativeLayout>
        
    </RelativeLayout>
    
    </RelativeLayout>
</LinearLayout>

